{
  "name": "@sudoo/build-cli",
  "version": "1.3.0",
  "description": "Build CLI for Sudoo Packages",
  "bin": {
    "sudoo-build": "cli",
    "sdb": "cli"
  },
  "directories": {
    "doc": "docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SudoDotDog/Sudoo-Build-CLI.git"
  },
  "author": "WMXPY",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SudoDotDog/Sudoo-Build-CLI/issues"
  },
  "homepage": "https://github.com/SudoDotDog/Sudoo-Build-CLI#readme",
  "dependencies": {
    "@sudoo/build-utils": "^1.0.0",
    "@sudoo/license": "^2.1.0"
  },
  "devDependencies": {
    "@sudoo/triforce": "^3.5.1"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "instrument": true,
    "reporter": [
      "json",
      "text-summary"
    ],
    "report-dir": "coverage",
    "sourceMap": true
  }
}